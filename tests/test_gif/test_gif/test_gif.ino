void setup() {
  strip.begin();
  strip.show();
}

void loop() {
  for (int frame = 0; frame < sizeof(gif_frames) / sizeof(gif_frames[0]); frame++) {
    displayFrame(gif_frames[frame]);
    delay(DELAY_TIME);
  }
}

void displayFrame(uint8_t frame[]) {
  for (int i = 0; i < NUM_LEDS; i++) {
    uint8_t r = frame[i * 3];
    uint8_t g = frame[i * 3 + 1];
    uint8_t b = frame[i * 3 + 2];
    strip.setPixelColor(i, r, g, b);
  }
  strip.show();
}